<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                  prism:ViewModelLocator.AutowireViewModel="True"
                  x:Class="Vnap.Views.LeftMenu">
  <MasterDetailPage.Master>
    <ContentPage Title="Default" BackgroundColor="#289B47">
      <StackLayout>
        <Button Text="ViewA" Command="{Binding NavigateCommand}" CommandParameter="Navigation/MainPage?tab=A" />
        <Button Text="ViewB" Command="{Binding NavigateCommand}" CommandParameter="Navigation/MainPage?tab=B" />
        <Button Text="ViewC" Command="{Binding NavigateCommand}" CommandParameter="BPage" />
      </StackLayout>
    </ContentPage>
  </MasterDetailPage.Master>
</MasterDetailPage>